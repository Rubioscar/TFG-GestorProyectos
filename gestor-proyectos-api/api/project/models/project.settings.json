{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "name": "Project"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "descripcion": {
      "type": "richtext"
    },
    "cerrado": {
      "type": "boolean"
    },
    "users": {
      "plugin": "users-permissions",
      "collection": "user",
      "via": "projects",
      "dominant": true
    },
    "admins": {
      "model": "grupo",
      "via": "project"
    },
    "issues": {
      "collection": "issue",
      "via": "project"
    },
    "tags": {
      "type": "json"
    },
    "work_flow": {
      "model": "work-flow"
    }
  }
}
